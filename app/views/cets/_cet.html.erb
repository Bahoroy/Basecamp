<div id="<%= dom_id cet %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Project:</strong>
    <%= cet.project_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this cet", cet, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this cet', edit_cet_path(cet), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
